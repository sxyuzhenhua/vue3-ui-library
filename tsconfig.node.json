{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
      "composite": true,
      "lib": ["ESNext"],
      "types": ["node"],
      "skipLibCheck": true
    },
    "include": [
      "internal/**/*",
      "internal/**/*.json",
      "scripts/**/*",
      "packages/theme-chalk/*",
      "packages/yu-element/version.ts",
      "packages/yu-element/package.json"
    ],
    "exclude": ["**/__tests__/**", "**/tests/**", "**/dist"]
  }
  